<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Secuencia para Proyectos Tracker -->
        <record id="seq_tracker_project" model="ir.sequence">
            <field name="name">Proyecto Tracker</field>
            <field name="code">tracker.project</field>
            <field name="prefix">TRK/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
        
        <!-- Secuencia para Tareas Tracker -->
        <record id="seq_tracker_task" model="ir.sequence">
            <field name="name">Tarea Tracker</field>
            <field name="code">tracker.task</field>
            <field name="prefix">TASK/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
        
    </data>
</odoo>