<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <record id="view_fill_rate_tree" model="ir.ui.view">
        <field name="name">fill.rate.tree</field>
        <field name="model">fill.rate.report</field>
        <field name="arch" type="xml">
            <tree>
                <field name="create_date"/>
                <field name="requisition_name"/>
                <field name="product_name"/>
                <field name="demanda"/>
                <field name="cantidad_entregada"/>
                <field name="fill_rate"/>
                <field name="uom_id"/>
            </tree>
        </field>
    </record>

    <record id="view_fill_rate_form" model="ir.ui.view">
        <field name="name">fill.rate.form</field>
        <field name="model">fill.rate.report</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="requisition_id"/>
                        <field name="product_id"/>
                        <field name="demanda"/>
                        <field name="cantidad_entregada"/>
                        <field name="fill_rate"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_fill_rate" model="ir.actions.act_window">
        <field name="name">Fill Rate</field>
        <field name="res_model">fill.rate.report</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_reports"
              name="Reportes"
              parent="employee_purchase_requisition.menu_purchase_request_pro"
              sequence="100"/>

    <menuitem id="menu_fill_rate"
              name="Fill Rate"
              parent="menu_reports"
              action="action_fill_rate"
              sequence="10"/>

</odoo>